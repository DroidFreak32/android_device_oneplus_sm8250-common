<resources>

    <!-- Max network scan search time in seconds -->
    <integer name="config_network_scan_helper_max_search_time_sec">254</integer>

    <!-- Whether or not the device is capable of multiple levels of vibration intensity.
         Note that this is different from whether it can control the vibration amplitude as some
         devices will be able to vary their amplitude but do not possess enough dynamic range to
         have distinct intensity levels -->
    <bool name="config_vibration_supports_multiple_intensities">true</bool>

    <!-- Battery Health -->
    <bool name="config_supportBatteryHealth">true</bool>
    <string name="config_batDesCap">/sys/class/power_supply/battery/charge_full_design</string>
    <integer name="config_batDesCapDivider">1000</integer>
    <string name="config_batCurCap">/sys/class/power_supply/battery/charge_full</string>
    <integer name="config_batCurCapDivider">1</integer>
    <string name="config_batChargeCycle">/sys/class/power_supply/battery/cycle_count</string>
    <string name="config_batHealthPercent">/sys/class/power_supply/bms/battery_h</string>
</resources>
